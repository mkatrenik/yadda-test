{
  "name": "cucumber-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "main": "test/runner.js",
  "scripts": {
    "test": "mocha test/support/init.js --require babel/register --reporter=spec --timeout=5000 --require=chai --no-timeouts",
    "test:debug": "node-debug mocha test/support/init.js --reporter=spec --require=chai --no-timeouts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.23",
    "chai": "~1.9.0",
    "deepmerge": "^0.2.7",
    "glob": "^4.4.0",
    "mocha": "^1.18.2",
    "webdriverio": "^3.2.0",
    "yadda": "^0.11.4"
  }
}
